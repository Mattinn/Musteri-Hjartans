<h1>Listing webstores</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Phone</th>
    <th>Address</th>
    <th>Link</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @webstores.each do |webstore| %>
  <tr>
    <td><%= webstore.title %></td>
    <td><%= webstore.description %></td>
    <td><%= webstore.phone %></td>
    <td><%= webstore.address %></td>
    <td><%= webstore.link %></td>
    <td><%= link_to 'Show', webstore %></td>
    <td><%= link_to 'Edit', edit_webstore_path(webstore) %></td>
    <td><%= link_to 'Destroy', webstore, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Webstore', new_webstore_path %>
