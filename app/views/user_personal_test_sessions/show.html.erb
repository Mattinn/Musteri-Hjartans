<p id="notice"><%= notice %></p>

<div class="fields">
<p>
  <b><%= @user_personal_test_session.personal_test.name %></b>
</p>
<p>
  <b><%= @user_personal_test_session.personal_test.description %></b>
</p>
<p>
  <% @user_personal_test_session.personal_test.questions.each do |q| %> 
  	<b><%= check_box_tag :answer  %> <%= q.question_text %> </b> </br> 
  <% end %>
</p>
  

</div>
<p>
  <b>User:</b>
  <%= @user_personal_test_session.user_id %>
</p>

<p>
  <b>Personal test:</b>
  <%= @user_personal_test_session.personal_test.name %>
</p>

<p>
  <b>Question:</b>
  <% @user_personal_test_session.personal_test.questions.each do |q| %>
  <%= q.question_text %>
  <br/>
  <%= q.answer %>
  <% end %>
</p>

<p>
  <b>Category:</b>
  <%= @user_personal_test_session.category_id %>
</p>

<p>
  <b>Treatment:</b>
  <%= @user_personal_test_session.treatment_id %>
</p>

<p>
  <b>Result:</b>
  <%= @user_personal_test_session.result_id %>
</p>


<%= link_to 'Edit', edit_user_personal_test_session_path(@user_personal_test_session) %> |
<%= link_to 'Back', user_personal_test_sessions_path %>
