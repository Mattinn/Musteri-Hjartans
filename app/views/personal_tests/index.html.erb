<h1>Listing personal_tests</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Result</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @personal_tests.each do |personal_test| %>
  <tr>
    <td><%= personal_test.name %></td>
    <td><%= personal_test.description %></td>
    <td><%= personal_test.result %></td>
    <td><%= link_to 'Show', personal_test %></td>
    <td><%= link_to 'Edit', edit_personal_test_path(personal_test) %></td>
    <td><%= link_to 'Destroy', personal_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

